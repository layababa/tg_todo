{
  "framework": "vite",
  "buildCommand": "cd web && npm run build",
  "outputDirectory": "web/dist",
  "installCommand": "cd web && npm install",
  "regions": ["sfo1"],
  "rewrites": [
    { "source": "/((?!assets/|favicon.ico|vite.svg).*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://ddddapi.zcvyzest.xyz/api"
  }
}

